{
    "name": "litecms/block",
    "description": "Block package for litecms.",
  "keywords": [
    "lavalite",
    "Block"
  ],
    "license": "MIT",
  "type": "lavalite-package",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Litecms\\Block\\": "src",
            "Litecms\\Block\\Seeders\\": "database/seeders"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Litecms\\Block\\Providers\\BlockServiceProvider"
            ],
            "aliases": {
                "Block" : "Litecms\\Block\\Facades\\Block"
            }
        }
    },
    "minimum-stability": "dev"
}
