{
  "name": "litecms/block",
  "description": "Block package for litecms.",
  "keywords": [
    "lavalite",
    "Block"
  ],
  "license": "MIT",
  "type": "lavalite-package",
  "authors": [
    {
      "name": "Lavalite",
      "email": "info@lavalite.org"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "composer/installers": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Litecms\\Block\\": "src",
      "Litecms\\Block\\Seeds\\": "database/seeds"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Litecms\\Block\\Providers\\BlockServiceProvider"
      ],
      "aliases": {
        "Block": "Litecms\\Block\\Facades\\Block"
      }
    }
  },
  "minimum-stability": "dev"
}
