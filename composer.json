{
    "name": "litecms/block",
    "description": "Block package for litecms.",
    "keywords": ["lavalite", "Block"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Litecms\\Block\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Litecms\\Block\\Providers\\BlockServiceProvider"
            ],
            "aliases": {
                "Block"       : "Litecms\\Block\\Facades\\Block"
            }
        }
    },
    "minimum-stability": "dev"
}
